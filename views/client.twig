# OpenVPN Client Configuration for {{ cn|e }}

client
dev tun
proto udp
tls-client
mlock

{% for remote in remotes %}
remote {{ remote|e }}
{% endfor %}

nobind
persist-key
persist-tun
remote-cert-tls server
comp-lzo
verb 3
key-direction 1
script-security 1

<ca>
{{ ca|e }}
</ca>
<cert>
{{ cert|e }}
</cert>
<key>
{{ key|e }}
</key>
<tls-auth>
{{ ta|e }}
</tls-auth>
