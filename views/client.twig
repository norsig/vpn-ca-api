# OpenVPN Client Configuration for {{ cn|e }}

client
dev tun
tls-client
mlock
nobind
persist-key
persist-tun
remote-cert-tls server
comp-lzo
verb 3
auth SHA256
key-direction 1
script-security 1
remote-random

{% for remote in remotes %}
remote {{ remote|e }}
{% endfor %}

<ca>
{{ ca|e }}
</ca>
<cert>
{{ cert|e }}
</cert>
<key>
{{ key|e }}
</key>
<tls-auth>
{{ ta|e }}
</tls-auth>
