# CA backend
caBackend: EasyRsa3Ca

# API credentials
Api:
    - 11223344556677889900

EasyRsa3Ca:
    # Fedora/CentOS (default)
    easyRsaPath: /usr/share/easy-rsa/3.0
    caPath: /var/lib/vpn-config-api/easy-rsa
    openVpnPath: /usr/sbin/openvpn
    CA:
        key_size: '4096'
        ca_expire: '1826'
        cert_expire: '365'
        ca_cn: VPN CA
